{
  "$schema": "../dist/config.schema.json",
  "appRepoPath": "/home/berlysia/workspace/agent-orchestorator",
  "agentCoordPath": "/home/berlysia/workspace/agent-orchestorator-coord",
  "maxWorkers": 5,
  "agents": {
    "planner": {
      "type": "claude",
      "model": "claude-opus-4-5"
    },
    "worker": {
      "type": "claude",
      "model": "claude-sonnet-4-5"
    },
    "judge": {
      "type": "claude",
      "model": "claude-haiku-4-5"
    }
  },
  "checks": {
    "enabled": true,
    "failureMode": "block"
  },
  "integration": {
    "method": "auto",
    "postIntegrationEvaluation": true,
    "maxAdditionalTaskIterations": 3,
    "mergeStrategy": "ff-prefer"
  },
  "planning": {
    "qualityThreshold": 60,
    "strictContextValidation": false,
    "maxTaskDuration": 2,
    "maxTasks": 20,
    "planQualityJudge": {
      "type": "codex",
      "model": "gpt-5.2-codex"
    }
  },
  "iterations": {
    "plannerQualityRetries": 5,
    "judgeTaskRetries": 5,
    "orchestrateMainLoop": 5,
    "serialChainTaskRetries": 5
  },
  "commit": {
    "autoSignature": false,
    "integrationSignature": true
  }
}
