{
  "$schema": "../dist/config.schema.json",
  "appRepoPath": ".",
  "agentCoordPath": "../agent-orchestorator-coord",
  "maxWorkers": 3,
  "agents": {
    "planner": {
      "type": "claude",
      "model": "claude-opus-4-5"
