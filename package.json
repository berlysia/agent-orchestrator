{
  "name": "agent-orchestrator",
  "version": "0.1.0",
  "description": "Multi-agent collaborative development tool with Planner/Worker/Judge architecture",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agent": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsgo --noEmit",
    "compile": "tsgo",
    "test": "node --test tests/unit/**/*.test.ts",
    "test:e2e": "node --test tests/e2e/**/*.test.ts",
    "test:all": "node --test tests/**/*.test.ts",
    "dev": "node --watch src/cli/index.ts",
    "lint": "oxlint src/",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "agent",
    "orchestrator",
    "multi-agent",
    "worktree",
    "claude",
    "codex"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=24.13.0"
  },
  "packageManager": "pnpm@10.28.0",
  "devDependencies": {
    "@types/node": "25.0.6",
    "@typescript/native-preview": "7.0.0-dev.20260111.1",
    "oxlint": "1.38.0",
    "prettier": "3.7.4"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "0.2.4",
    "@openai/codex-sdk": "0.80.0",
    "commander": "14.0.2",
    "option-t": "55.1.0",
    "simple-git": "3.30.0",
    "zod": "4.3.5"
  }
}
